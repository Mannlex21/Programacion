    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^2.5.9",
    "vue-jest": "^2.3.0",
    "vue-loader": "^11.3.4",
    "vue-property-decorator": "^5.3.0",
    "vue-router": "^2.8.1",
